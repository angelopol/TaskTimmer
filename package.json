{
  "name": "tasktimmer",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
  "migrate:deploy": "prisma migrate deploy",
  "migrate:prod": "npm run prisma:generate && npm run migrate:deploy",
    "seed": "tsx prisma/seed.ts",
    "fix:timestamps": "ts-node -P tsconfig.scripts.json scripts/fixLocalTimestamps.ts",
    "migrate:fix-dates": "ts-node -P tsconfig.scripts.json scripts/migrations/fix_timelog_dates.ts",
    "migrate:fix-dates:apply": "set APPLY=1&& ts-node -P tsconfig.scripts.json scripts/migrations/fix_timelog_dates.ts",
    "generate:icons": "node scripts/generate-icons.js"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@prisma/client": "latest",
    "bcrypt": "^5.1.1",
    "framer-motion": "^12.23.12",
    "next": "^14.2.32",
    "next-auth": "^4.24.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.62.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/node": "20.12.7",
    "@types/react": "18.2.22",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "prisma": "latest",
    "sharp": "^0.33.2",
    "tailwindcss": "^3.4.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "typescript": "5.4.5"
  }
}
